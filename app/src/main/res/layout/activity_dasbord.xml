   <LinearLayout
       android:id="@+id/lv1"
       android:gravity="center"
       android:layout_marginTop="10sp"
       android:layout_below="@+id/lv"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="horizontal"
       android:background="@drawable/dasboard_shape">
      <TextView
           android:gravity="center"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textColor="@color/colorwhite"
           android:drawableLeft="@drawable/ic_dashboard_black_24dp"
           android:textSize="20sp"
           android:text="Dashboard"/>
   </LinearLayout>
   <LinearLayout
       android:id="@+id/lv2"
       android:layout_below="@+id/lv1"
       android:layout_marginLeft="5sp"
       android:layout_marginRight="5sp"
       android:layout_width="fill_parent"
       android:layout_height="wrap_content"
       android:orientation="horizontal"
       android:weightSum="5"
       >
      <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
          android:id="@+id/card_viewprofile"
          android:layout_width="184dp"
          android:layout_height="175dp"
          android:layout_weight="2.50"
          android:layout_gravity="center"
          card_view:cardCornerRadius="4dp">
          <ImageView
              android:layout_gravity="center"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:background="@drawable/profile"

              />
         <TextView
             android:layout_marginTop="140sp"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:textSize="15sp"
             android:gravity="center"
             android:textStyle="bold"
             android:text="MY PROFILE"/>

      </android.support.v7.widget.CardView>
      <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
          android:id="@+id/card_viewChangePassword"
          android:layout_width="187dp"
          android:layout_height="175dp"
          android:layout_weight="2.50"
          android:layout_gravity="right"
          card_view:cardCornerRadius="4dp"
          >

         <ImageView
             android:layout_gravity="center"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:background="@drawable/changepassword"

             />
         <TextView
             android:layout_marginTop="140sp"
