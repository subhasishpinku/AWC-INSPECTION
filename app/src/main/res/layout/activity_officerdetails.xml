<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:fillViewport="true">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/lv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#26649A"
            android:orientation="vertical">
            <LinearLayout
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <android.support.v7.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="250sp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:background="#26649A"
                    android:minHeight="?attr/actionBarSize"
                    app:theme="@style/ThemeOverlay.AppCompat.Dark"
                    card_view:ignore="RtlCompat" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="60sp"
                    android:layout_weight="1"
                    android:src="@mipmap/logo" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lv1"
            android:gravity="center"
            android:layout_marginTop="10sp"
            android:layout_below="@+id/lv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/dasboard_shape">
            <TextView
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorwhite"
                android:drawableLeft="@drawable/ic_profile"
                android:textSize="20sp"
                android:text="Officer Details"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lv2"
            android:layout_below="@+id/lv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@+id/card_view"
                android:layout_marginTop="10sp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_marginLeft="@dimen/inspection_left"
                android:layout_marginRight="@dimen/inspection_right"
                android:layout_height="140sp"
                card_view:cardCornerRadius="@dimen/cardview_cardCornerRadius">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/awcstvId"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:text="YUBA SHAKTI CLUB (19338030102 )"
                        android:background="@drawable/edit_text_shape"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="60sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:text="District"/>
                    <TextView
                        android:layout_marginLeft="40sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=":"/>
                    <TextView
                        android:id="@+id/districId"
                        android:layout_marginLeft="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorblack"
                        android:text="Hooghly"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="80sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:text="Project "/>
                    <TextView
                        android:layout_marginLeft="40sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=":"/>
                    <TextView
                        android:id="@+id/projectID"
                        android:layout_marginLeft="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorblack"
                        android:text="Baidyabati"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="100sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:text="Sector  "/>
                    <TextView
                        android:layout_marginLeft="40sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=":"/>
                    <TextView
                        android:id="@+id/sectorID"
                        android:layout_marginLeft="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorblack"
                        android:text="Sector1"/>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lv3"
            android:layout_below="@+id/lv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <android.support.v7.widget.CardView
            android:id="@+id/card_view1"
            android:layout_marginTop="10sp"
            android:layout_gravity="center"
            android:layout_width="match_parent"
            android:layout_marginLeft="@dimen/inspection_left"
            android:layout_marginRight="@dimen/inspection_right"
            android:layout_height="140sp"
            card_view:cardCornerRadius="@dimen/cardview_cardCornerRadius">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tvId1"
                    android:layout_marginTop="10sp"
                    android:layout_width="match_parent"
                    android:layout_height="40sp"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:text="CDPO Details"
                    android:background="@drawable/edit_text_shape"
                    />
                <LinearLayout
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="40sp"
                        android:layout_height="40sp"
                        android:background="@drawable/edit_text_shape"
                        android:gravity="center"
                        android:padding="6sp"
                        android:src="@drawable/ic_userprofile"  />
                    <TextView
                        android:id="@+id/cdpoName"
                        android:layout_marginLeft="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingLeft="5sp"
                        android:text="CDPO Name"
                        android:textColor="@color/colorblack"
                        android:background="@drawable/edit_text_shape"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="5sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="40sp"
                        android:layout_height="40sp"
                        android:background="@drawable/edit_text_shape"
                        android:gravity="center"
                        android:padding="6sp"
                        android:src="@drawable/ic_phone_black_24dp" />
                    <TextView
                        android:id="@+id/cdpoNumber"
                        android:layout_marginLeft="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingLeft="5sp"
                        android:textColor="@color/colorblack"
                        android:text="Show Phone Number"
                        android:background="@drawable/edit_text_shape" />
                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lv4"
            android:layout_below="@+id/lv3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@+id/card_view2"
                android:layout_marginTop="10sp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_marginLeft="@dimen/inspection_left"
                android:layout_marginRight="@dimen/inspection_right"
                android:layout_height="140sp"
                card_view:cardCornerRadius="@dimen/cardview_cardCornerRadius">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvId2"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:text="ACDPO Details"
                        android:background="@drawable/edit_text_shape"
                        />
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_userprofile"/>
                        <TextView
                            android:id="@+id/acdpoName"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="ACDPO Name"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_phone_black_24dp" />
                        <TextView
                            android:id="@+id/acdpoNumber"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="Show Phone Number"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lv5"
            android:layout_below="@+id/lv4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@+id/card_view3"
                android:layout_marginTop="10sp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_marginLeft="@dimen/inspection_left"
                android:layout_marginRight="@dimen/inspection_right"
                android:layout_height="140sp"
                card_view:cardCornerRadius="@dimen/cardview_cardCornerRadius">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvId3"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:text="Supervisor Details"
                        android:background="@drawable/edit_text_shape"
                        />
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_userprofile"  />
                        <TextView
                            android:id="@+id/supervisorName"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="CDPO Name"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_phone_black_24dp" />
                        <TextView
                            android:id="@+id/supervisorNumber"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="Show Phone Number"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lv6"
            android:layout_below="@+id/lv5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@+id/card_view4"
                android:layout_marginTop="10sp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_marginLeft="@dimen/inspection_left"
                android:layout_marginRight="@dimen/inspection_right"
                android:layout_height="140sp"
                card_view:cardCornerRadius="@dimen/cardview_cardCornerRadius">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvId4"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:text="Worker Details"
                        android:background="@drawable/edit_text_shape"
                        />
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_userprofile"  />
                        <TextView
                            android:id="@+id/workerName"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="Workar Name"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_phone_black_24dp" />
                        <TextView
                            android:id="@+id/workerNumber"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="Show Phone Number"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lv7"
            android:layout_below="@+id/lv6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <android.support.v7.widget.CardView
                android:id="@+id/card_view5"
                android:layout_marginTop="10sp"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_marginLeft="@dimen/inspection_left"
                android:layout_marginRight="@dimen/inspection_right"
                android:layout_height="140sp"
                card_view:cardCornerRadius="@dimen/cardview_cardCornerRadius">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvId5"
                        android:layout_marginTop="10sp"
                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:text="Helper Details"
                        android:background="@drawable/edit_text_shape"
                        />
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_userprofile"  />

                        <TextView
                            android:id="@+id/helperName"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="Helper Name"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="5sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="40sp"
                            android:layout_height="40sp"
                            android:background="@drawable/edit_text_shape"
                            android:gravity="center"
                            android:padding="6sp"
                            android:src="@drawable/ic_phone_black_24dp" />
                        <TextView
                            android:id="@+id/helperNumber"
                            android:layout_marginLeft="10sp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="5sp"
                            android:text="Show Phone Number"
                            android:textColor="@color/colorblack"
                            android:background="@drawable/edit_text_shape" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>
    </RelativeLayout>
</ScrollView>
